<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="472.0" prefWidth="773.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ConfigManager">
   <children>
      <Button fx:id="loadBtn" layoutX="37.0" layoutY="423.0" mnemonicParsing="false" onAction="#loadMeta" onMouseEntered="#hooverLoad" onMouseExited="#exitLoad" prefHeight="26.0" prefWidth="72.0" text="  Load" />
      <Button fx:id="adderBtn" layoutX="126.0" layoutY="423.0" mnemonicParsing="false" onAction="#showAdder" onMouseEntered="#hooverAdder" onMouseExited="#exitAdder" prefHeight="26.0" prefWidth="99.0" text="    Add New" />
      <Button fx:id="editBtn" layoutX="245.0" layoutY="423.0" mnemonicParsing="false" onAction="#showEditor" onMouseEntered="#hooverEdit" onMouseExited="#exitEdit" prefHeight="26.0" prefWidth="72.0" text="Edit" />
      <Button fx:id="returnBtn" layoutX="462.0" layoutY="423.0" mnemonicParsing="false" onAction="#goBack" onMouseEntered="#hooverReturn" onMouseExited="#exitReturn" prefHeight="26.0" prefWidth="72.0" text="  Return" />
      <Button fx:id="removeBtn" layoutX="344.0" layoutY="423.0" mnemonicParsing="false" onAction="#removeMeta" onMouseEntered="#hooverRemove" onMouseExited="#exitRemove" prefHeight="26.0" prefWidth="85.0" text="  Remove" />
      <TableView fx:id="musicTable" layoutX="-1.0" prefHeight="412.0" prefWidth="773.0">
        <columns>
          <TableColumn fx:id="nameCol" prefWidth="147.0" text="Name" />
          <TableColumn fx:id="artistCol" prefWidth="170.0" text="Artist" />
            <TableColumn fx:id="albumCol" prefWidth="177.0" text="Album" />
            <TableColumn fx:id="yearCol" text="Year" />
            <TableColumn fx:id="genreCol" prefWidth="84.0" text="Genre" />
            <TableColumn fx:id="durationCol" prefWidth="94.0" text="Duration(s)" />
        </columns>
      </TableView>
      <FontAwesomeIconView fx:id="loadIcon" glyphName="ARROW_CIRCLE_UP" layoutX="46.0" layoutY="440.0" />
      <FontAwesomeIconView fx:id="adderIcon" glyphName="PLUS_SQUARE" layoutX="133.0" layoutY="440.0" />
      <FontAwesomeIconView fx:id="editIcon" glyphName="EDIT" layoutX="253.0" layoutY="440.0" />
      <FontAwesomeIconView fx:id="removeIcon" glyphName="MINUS_CIRCLE" layoutX="351.0" layoutY="440.0" />
      <FontAwesomeIconView fx:id="returnIcon" glyphName="ARROW_CIRCLE_LEFT" layoutX="468.0" layoutY="440.0" />
   </children>
</AnchorPane>
