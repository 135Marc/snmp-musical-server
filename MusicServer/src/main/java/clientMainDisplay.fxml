<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="330.0" prefWidth="482.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientMainDisplay">
   <children>
      <TableView fx:id="agentTableView" layoutX="20.0" layoutY="16.0" onMouseEntered="#loadAgents" prefHeight="240.0" prefWidth="416.0" />
      <Button fx:id="addBtn" layoutX="38.0" layoutY="278.0" mnemonicParsing="false" onAction="#addAgent" onMouseEntered="#hooverAdd" onMouseExited="#exitAdd" prefHeight="33.0" prefWidth="123.0" text="    New Agent" />
      <Button fx:id="acessBtn" layoutX="206.0" layoutY="279.0" mnemonicParsing="false" onAction="#acessAgent" onMouseEntered="#hooverAcess" onMouseExited="#exitAcess" prefHeight="33.0" prefWidth="89.0" text=" Acess" />
      <Button fx:id="exitBtn" layoutX="347.0" layoutY="278.0" mnemonicParsing="false" onAction="#close" onMouseEntered="#hooverExit" onMouseExited="#exitClose" prefHeight="33.0" prefWidth="89.0" text="Exit" />
      <FontAwesomeIconView fx:id="addIcon" glyphName="PLUS_CIRCLE" layoutX="51.0" layoutY="300.0" size="14.0" />
      <FontAwesomeIconView fx:id="acessIcon" glyphName="SERVER" layoutX="213.0" layoutY="302.0" size="14.0" />
      <FontAwesomeIconView fx:id="exitIcon" glyphName="CLOSE" layoutX="360.0" layoutY="300.0" size="14" />
   </children>
</AnchorPane>
